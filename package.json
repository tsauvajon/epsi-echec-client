  "dependencies": {
    "babel": "^6.23.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "codecov": "^2.1.0",
    "cross-env": "^5.0.0",
    "deep-freeze": "^0.0.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
