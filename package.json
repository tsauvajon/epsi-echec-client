{
  "name": "epsi-echec-client",
  "version": "0.1.0",
  "homepage": "https://tsauvajon.github.io/epsi-echec-client",
  "private": true,
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.2"
  },
  "dependencies": {
    "codecov": "^2.1.0",
    "deep-freeze": "^0.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gh-pages": "^0.12.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-scripts": "0.9.0",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-full": "yarn lint && react-scripts test --env=jsdom --coverage",
    "test-once-windows": "set CI=true && yarn test",
    "test-once-unix": "CI=true yarn test",
    "eject": "react-scripts eject",
    "coverage": "codecov",
    "lint": "./node_modules/.bin/eslint ./ --cache --ignore-pattern .gitignore",
    "deploy": "gh-pages -d build"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test-once-windows"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/tsauvajon/epsi-echec-client.git",
  "author": "Thomas SAUVAJON <thomas.sauvajon@gmail.com>",
  "license": "MIT"
}
